{
	"name": "astro-cdn-loader",
	"version": "0.0.0",
	"description": "An Astro integration to load packages from a CDN like jsDelivr or unpkg.",
	"author": {
		"name": "anmoti",
		"url": "https://profile.anmoti.com"
	},
	"license": "MIT",
	"keywords": [
		"astro-integration",
		"astro-loader",
		"withastro",
		"astro",
		"jsdelivr",
		"cdn"
	],
	"homepage": "https://github.com/anmoti/astro-cdn-loader",
	"publishConfig": {
		"access": "public"
	},
	"sideEffects": false,
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup"
	},
	"type": "module",
	"peerDependencies": {
		"astro": "^5.1.3"
	},
	"dependencies": {
		"astro-integration-kit": "^0.19.0"
	},
	"devDependencies": {
		"tsup": "^8.5.0"
	}
}
